{
    "manifest_version": 2,
    "name": "LibGen Download Link",
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "author": "Saeed Moghadamzadeh",
    "version": "0.1",
    "description": "An extension to search in LibGen from GoodReads",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png"
    },
    "web_accessible_resources": [
        "icons/*"
    ]
}